# ./sonar_goto.db 

record(ao, "$(USER):goto"){
	field(DESC, "GOTO")
	field(DTYP , "stream")
	field(OUT , "@sonar_array.proto go_to $(PORT)")
}
# ./sonar_measure_pos.db 

record(ai, "$(USER):measure_pos"){
	field(DESC, "MEASURE POS")
	field(DTYP , "stream")
	field(SCAN , "1 second")
	field(INP , "@sonar_array.proto get_pos $(PORT)")
}
# .../Db/sonar_scan_dis.db

record(waveform , "$(USER):wf") {
	field(DESC, "SCAN DIS waveform")
	field(DTYP , "stream")
	field(SCAN , "1 second")
	field(NELM, "6")
	field(FTVL, "FLOAT")
	field(INP , "@sonar_array.proto get_dis_scan $(PORT)")
}
